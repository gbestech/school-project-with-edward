{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://school-project-with-edward.onrender.com/api/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "https://school-project-with-edward.onrender.com/media/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
